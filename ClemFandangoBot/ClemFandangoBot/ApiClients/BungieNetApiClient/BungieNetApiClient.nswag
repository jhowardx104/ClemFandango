},\r\n              {\r\n                \"numericValue\": \"3\",\r\n                \"identifier\": \"Strike\"\r\n              },\r\n              {\r\n                \"numericValue\": \"4\",\r\n                \"identifier\": \"Raid\"\r\n              },\r\n              {\r\n                \"numericValue\": \"5\",\r\n                \"identifier\": \"AllPvP\"\r\n              },\r\n              {\r\n                \"numericValue\": \"6\",\r\n                \"identifier\": \"Patrol\"\r\n              },\r\n              {\r\n                \"numericValue\": \"7\",\r\n                \"identifier\": \"AllPvE\"\r\n              },\r\n              {\r\n                \"numericValue\": \"9\",\r\n                \"identifier\": \"Reserved9\"\r\n              },\r\n              {\r\n                \"numericValue\": \"10\",\r\n                \"identifier\": \"Control\"\r\n              },\r\n              {\r\n                \"numericValue\": \"11\",\r\n                \"identifier\": \"Reserved11\"\r\n              },\r\n              {\r\n                \"numericValue\": \"12\",\r\n                \"identifier\": \"Clash\",\r\n                \"description\": \"Clash -> Destiny's name for Team Deathmatch. 4v4 combat, the team with the highest kills at the end of time wins.\"\r\n              },\r\n              {\r\n                \"numericValue\": \"13\",\r\n                \"identifier\": \"Reserved13\"\r\n              },\r\n              {\r\n                \"numericValue\": \"15\",\r\n                \"identifier\": \"CrimsonDoubles\"\r\n              },\r\n              {\r\n                \"numericValue\": \"16\",\r\n                \"identifier\": \"Nightfall\"\r\n              },\r\n              {\r\n                \"numericValue\": \"17\",\r\n                \"identifier\": \"HeroicNightfall\"\r\n              },\r\n              {\r\n                \"numericValue\": \"18\",\r\n                \"identifier\": \"AllStrikes\"\r\n            